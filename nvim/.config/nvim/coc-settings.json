{
  "clangd.semanticHighlighting": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "markdown",
    "python",
    "scss",
    "sql",
    "typescript",
    "typescriptreact",
    "xml",
    "go"
  ],
  "css.lint.argumentsInColorFunction": "error",
  "css.lint.boxModel": "warning",
  "css.lint.compatibleVendorPrefixes": "warning",
  "css.lint.duplicateProperties": "error",
  "css.lint.emptyRules": "warning",
  "css.lint.float": "warning",
  "css.lint.fontFaceProperties": "warning",
  "css.lint.hexColorLength": "warning",
  "css.lint.ieHack": "warning",
  "css.lint.important": "warning",
  "css.lint.importStatement": "warning",
  "css.lint.idSelector": "warning",
  "css.lint.propertyIgnoredDueToDisplay": "warning",
  "css.lint.universalSelector": "warning",
  "css.lint.unknownAtRules": "warning",
  "css.lint.unknownProperties": "error",
  "css.lint.unknownVendorSpecificProperties": "error",
  "css.lint.vendorPrefix": "error",
  "css.lint.zeroUnits": "warning",
  "diagnostic.checkCurrentLine": false,
  "diagnostic.errorSign": "E",
  "diagnostic.hintSign": "H",
  "diagnostic.infoSign": "I",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.refreshAfterSave": true,
  "diagnostic.warningSign": "W",
  "html.autoClosingTags": true,
  "html.format.endWithNewline": true,
  "html.format.indentInnerHtml": true,
  "html.format.indentHandlebars": true,
  "html.format.wrapAttributes": "auto",
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "./UltiSnips"
  ],
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "suggest.noselect": false,
  "tsserver.log": "verbose",
  "tsserver.npm": "/usr/bin/npm",
  "xml.validation.noGrammar": "error"
}
